 <%= form_for @user do |f| %>

  <p>
  <%= f.label :name %>:
  <%= f.text_field :name %><br /> 
  </p>

  <p>
  <%= f.label :age %>:
  <%= f.text_field :age%><br /> 
  </p>

  <p>
  <%= f.label :email %>:
  <%= f.text_field :email %><br />
  </p>

  <p>
  <%= f.label :password %>:
  <%= f.password_field :password %><br />

  <%= f.submit %>

  <% end %>